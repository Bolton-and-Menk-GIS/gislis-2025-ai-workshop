{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build:type-check": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build": "vite build --emptyOutDir",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "build:openapi": "openapi-typescript http://localhost:8000/openapi.json -o src/generated/api.ts"
  },
  "dependencies": {
    "@arcgis/core": "^4.33.14",
    "@arcgis/map-components": "^4.33.23",
    "@crmackey/shp-write": "^0.4.5",
    "@picocss/pico": "^2.1.1",
    "@terraformer/arcgis": "^2.1.2",
    "@turf/turf": "^7.2.0",
    "@types/terraformer__arcgis": "^2.0.5",
    "@vueuse/core": "^13.9.0",
    "debug": "^4.4.3",
    "js-yaml": "^4.1.0",
    "loglevel": "^1.9.2",
    "openapi-typescript-fetch": "^2.2.1",
    "pdfjs-dist": "^5.4.149",
    "pinia": "^3.0.3",
    "uuid": "^13.0.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@prettier/plugin-oxc": "^0.0.4",
    "@tsconfig/node22": "^22.0.2",
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-oxlint": "~1.8.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.9.1",
    "oxlint": "~1.8.0",
    "prettier": "3.6.2",
    "sass-embedded": "^1.93.2",
    "typescript": "~5.8.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.4"
  },
  "volta": {
    "node": "22.12.0",
    "npm": "10.9.0"
  }
}
